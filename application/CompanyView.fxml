<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="437.0" prefWidth="774.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CompanyView">
   <children>
      <TableView fx:id="CompanyView" layoutX="142.0" layoutY="52.0" prefHeight="392.0" prefWidth="424.0">
         <columns>
            <TableColumn fx:id="Customer_ID" prefWidth="105.0" text="Customer ID" />
            <TableColumn fx:id="C_Name" onEditCommit="#EdiTme" prefWidth="105.0" text="Company Name" />
            <TableColumn fx:id="Address" onEditCommit="#EditAddress" prefWidth="105.0" text="Address" />
            <TableColumn fx:id="Phone" onEditCommit="#EditPhone" prefWidth="105.0" text="Phone" />
         </columns>
      </TableView>
      <HBox layoutX="19.0" layoutY="452.0" prefHeight="47.0" prefWidth="636.0">
         <children>
            <Button fx:id="Back" mnemonicParsing="false" onAction="#back" prefHeight="39.0" prefWidth="59.0" text="Back">
               <HBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="EditBt" mnemonicParsing="false" onAction="#Edit" prefHeight="35.0" prefWidth="59.0" text="Edit">
               <HBox.margin>
                  <Insets bottom="10.0" left="188.0" right="50.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="SaveBt" mnemonicParsing="false" onAction="#Save" prefHeight="35.0" prefWidth="59.0" text="Save">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
